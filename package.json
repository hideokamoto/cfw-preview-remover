{
  "name": "cwc",
  "version": "1.0.0",
  "description": "Cloudflare Workers preview deploymentを安全に削除するCLIツール",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cwc": "./bin/cwc"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "deployment",
    "preview",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "dotenv": "^16.4.0",
    "ora": "^8.0.0",
    "wrangler": "^3.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.50.0"
  }
